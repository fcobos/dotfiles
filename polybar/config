;----------------------------------------------------------
;		      S C R I P T    M O D U L E S
;    https://github.com/jaagr/polybar/wiki/Module:-script
;----------------------------------------------------------

; Example json files for mail, weather, & music: https://github.com/Chaosteil/dotfiles/tree/master/polybar/.config/polybar

[module/mail]
type = custom/script
exec = $HOME/.config/polybar/mail.py
interval = 120
label = " ÔÄú %output:0:5:...% "
; ÔÄú ÔÉ†
format-foreground = ${colors.foreground}
format-background = ${colors.red}

[module/weather]
type = custom/script
exec = $HOME/.config/polybar/weather.py
interval = 120
label = " ÔÉÇ %output:0:22:...% "
format-foreground = ${colors.red}
format-background = ${colors.foreground}

[module/music]
type = custom/script
exec = $HOME/.config/polybar/mediaplayer.py
exec-if = pgrep -x spotify
interval = 3
label = " ÔÜº %output:0:30:ÔÖÅ% "
format-foreground = ${colors.foreground}
format-background = ${colors.red}
click-left = dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause

[module/ssh]
type = custom/script
exec = echo + SSH
exec-if = pgrep ssh
interval = 3.0
format-foreground = ${colors.gray}
format-background = ${colors.foreground}
label = " %output:0:5%"
click-left = echo left %counter%
click-middle = echo middle %counter%
click-right = echo right %counter%
scroll-up = echo scroll up %counter%
scroll-down = echo scroll down %counter%

[module/vpn]
type = custom/script
exec = echo + VPN
exec-if = pgrep vpn
interval = 3.0
format-foreground = ${colors.gray}
format-background = ${colors.foreground}
label = " %output:0:5%"
click-left = echo left %counter%
click-middle = echo middle %counter%
click-right = echo right %counter%
scroll-up = echo scroll up %counter%
scroll-down = echo scroll down %counter%

[module/btc]
type = custom/script
exec = curl -s https://apiv2.bitcoinaverage.com/indices/global/ticker/BTCUSD | python -c "import json, sys; print(json.load(sys.stdin)['last'])"
interval = 60.0
format-foreground = ${colors.red}
format-background = ${colors.foreground}
label = " $%output:0:10% "
;label = " BTC:$%output:0:10% "

[module/etherium]
type = custom/script
exec = curl -s https://apiv2.bitcoinaverage.com/indices/global/ticker/ETHUSD | python -c "import json, sys; print(json.load(sys.stdin)['last'])"
interval = 60.0
format-foreground = ${colors.foreground}
format-background = ${colors.red}
label = " $%output:0:7% "
;label = " ETH:$%output:0:7% "


