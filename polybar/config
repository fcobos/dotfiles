[module/powermenu]
type = custom/menu

; ÔÅì ÔÅî ÔÑÑ ÔÑÖ ÔÖº ¬ª 
;label-open = "ÔÅî "
label-open = "  ÔÄë  "
label-open-font = 3
label-open-foreground = ${colors.foreground}
label-open-background = ${colors.background}
label-close = 
label-close-foreground = ${colors.foreground}
label-close-background = ${colors.black}
label-separator = " "
label-separator-foreground = ${colors.white}
label-separator-background = ${colors.black}

; lock
menu-0-0 = "ÔÄ£"
menu-0-0-exec = xflock4
menu-0-0-foreground = ${colors.foreground}
menu-0-0-background = ${colors.black}
; sleep
menu-0-1 = "Ôà∂"
menu-0-1-exec = zenity --question --text="Suspend?" --icon-name=system-suspend --default-cancel && xflock4 && systemctl suspend
menu-0-1-foreground = ${colors.foreground}
menu-0-1-background = ${colors.black}
; logout
menu-0-2 = "ÔÇã"
menu-0-2-exec = zenity --question --text="Logout?" --icon-name=gnome-logout --default-cancel && (killall -9 pulseaudio ; i3-msg exit)
menu-0-2-foreground = ${colors.foreground}
menu-0-2-background = ${colors.black}
; reboot
menu-0-3 = "ÔÄ°"
menu-0-3-exec = zenity --question --text="Reboot?" --icon-name=system-reboot --default-cancel && systemctl reboot
menu-0-3-foreground = ${colors.foreground}
menu-0-3-background = ${colors.black}
; shutdown
menu-0-4 = "ÔÄë"
menu-0-4-exec = zenity --question --text="Shutdown?" --icon-name=system-shutdown --default-cancel && systemctl poweroff
menu-0-4-foreground = ${colors.foreground}
menu-0-4-background = ${colors.black}
; end stripe
menu-0-5 = "ÔÅî "
menu-0-5-font = 3
menu-0-5-foreground = ${colors.white}
menu-0-5-background = ${colors.black}


